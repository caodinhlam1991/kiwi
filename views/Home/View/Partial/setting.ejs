                <div class="dLine">
                    <label for="iMnuExcept<%- value.id; %>"><%- $.trans.gettext('Description'); %>:</label>
                    <textarea name="except<%- value.id; %>" id="iMnuExcept<%- value.id; %>"></textarea>
                </div>
                <textarea name="MnuDetail<%- value.id; %>" placeholder="<%- $.trans.gettext('detail'); %>"></textarea>
                <div class="dPhoto"></div>
                <a class="a1Photo" board="dBoardAddMenu"><img alt="wsup3" src="<%- $.config.skin; %>/admin/photo.png"/> <%- $.trans.gettext('Add photos'); %></a>
                <a class="aFeature" type="feature" num="0" board="dBoardAddLanguage"><%- $.trans.gettext('Add feature'); %></a>
                <div class="dFeatures"></div>
            </div>
            <%
            });
            %>
            <div class="dAdvance">
                <div>
                    <div class="dLine">
                        <label for="iMnuPos"><%- $.trans.gettext('Position'); %>: </label>
                        <input type="text" name="position" id="iMnuPos"/>
                    </div>
                    <div class="dLine">
                        <label for="iMnuKey"><%- $.trans.gettext('Key'); %>:</label>
                        <input type="text" name="path" id="iMnuKey"/>
                    </div>
                    <select class="iLive">
                        <option value="true"><%- $.trans.gettext('Public'); %></option>
                        <option value="false"><%- $.trans.gettext('Private'); %></option>
                    </select>
                </div>
            </div>
            <a class="aAdvance" title="<%- $.trans.gettext('Advance'); %>"><%- $.trans.gettext('Advance'); %></a>
        </form>
    </div>
</div>