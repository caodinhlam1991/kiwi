                %>
                <a title="<%- value.name; %>"><img src="<%- value.image; %>" alt="Wsup3"/> <span><%- value.name; %></span></a>
                <%
                });
                %>
            </div>
            <%
            $.each(flags, function(key, value)
            {
            %>
            <div class="dTabsLanguage dTabsLanguage<%- value.id; %>">
