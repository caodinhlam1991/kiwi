<div class="dFrame">
    <div class="dBoardAction dBoardUser dShow">
        <div class="dFramePng">
            <div class="dAppPage">
                <div class="dNotify">
                    <span class="sBlur" style="background-image: url('../../public/media/images/skin/admin/customer.png')"></span>
                    <span class="sScreen"></span>
                    <span class="sNumber">6</span>
                </div>
                <div class="dClose" board="dBoardRegister">
                    <span class="sBlur" style="background-image: url('../../public/media/images/skin/admin/register.png')"></span>
                    <span class="sScreen"></span>
                    <span class="fa fa-times"></span>
                </div>
                <a title="<%- $.trans.gettext('Register'); %>">
                    <img src="<%- $.config.skin; %>/admin/register.png" alt="wsup3"/>
                </a>
                <a title="<%- $.trans.gettext('Register'); %>"><%- $.trans.gettext('Register'); %></a>
            </div>
            <div class="dAppPage">
                <div class="dNotify">
                    <span class="sBlur" style="background-image: url('../../public/media/images/skin/admin/customer.png')"></span>
                    <span class="sScreen"></span>
                    <span class="sNumber">6</span>
                </div>
                <div class="dClose" board="dBoardLogin">
                    <span class="sBlur" style="background-image: url('../../public/media/images/skin/admin/login.png')"></span>
                    <span class="sScreen"></span>
                    <span class="fa fa-times"></span>
                </div>
                <a title="<%- $.trans.gettext('Login'); %>">
                    <img src="<%- $.config.skin; %>/admin/login.png" alt="wsup3"/>
                </a>
                <a title="<%- $.trans.gettext('Login'); %>"><%- $.trans.gettext('Login'); %></a>
            </div>
            <div class="dAppPage">
                <div class="dNotify">
                    <span class="sBlur" style="background-image: url('../../public/media/images/skin/admin/customer.png')"></span>
                    <span class="sScreen"></span>
                    <span class="sNumber">6</span>
                </div>
                <div class="dClose" board="dBoardForgot">
                    <span class="sBlur" style="background-image: url('../../public/media/images/skin/admin/forgot.png')"></span>
                    <span class="sScreen"></span>
                    <span class="fa fa-times"></span>
                </div>
                <a title="<%- $.trans.gettext('Forgot password'); %>">
                    <img src="<%- $.config.skin; %>/admin/forgot.png" alt="wsup3"/>
                </a>
                <a title="<%- $.trans.gettext('Forgot password'); %>"><%- $.trans.gettext('Forgot password'); %></a>
            </div>
            <div class="dAppPage">
                <div class="dNotify">
                    <span class="sBlur" style="background-image: url('../../public/media/images/skin/admin/customer.png')"></span>
                    <span class="sScreen"></span>
                    <span class="sNumber">6</span>
                </div>
                <div class="dClose" board="dBoardSetting">
                    <span class="sBlur" style="background-image: url('../../public/media/images/skin/admin/setting.png')"></span>
                    <span class="sScreen"></span>
                    <span class="fa fa-times"></span>
                </div>
                <a title="<%- $.trans.gettext('Setting'); %>">
                    <img src="<%- $.config.skin; %>/admin/setting.png" alt="wsup3"/>
                </a>
                <a title="<%- $.trans.gettext('Setting'); %>"><%- $.trans.gettext('Setting'); %></a>
            </div>
        </div>
    </div>
    <div class="dBoardAction dBoardRegister">
        <div class="dFramePng">
            <form method="post" id="fRegister">
                <input type="email" id="iEmail" placeholder="<%- $.trans.gettext('Email'); %>" name="email"/>
                <input type="password" id="iPassword" placeholder="<%- $.trans.gettext('Password'); %>" name="password"/>
                <input type="password" id="iRePassword" placeholder="<%- $.trans.gettext('Retype password'); %>" name="repassword"/>
                <input type="text" id="iDisplayName" placeholder="<%- $.trans.gettext('Display name'); %>" name="displayname"/>
                <canvas id="cCaptcha" width="100" height="29"></canvas>
                <input type="text" id="iCaptcha" placeholder="<%- $.trans.gettext('Confirm code'); %>" name="captcha"/>
                <span class="sBtn"><input class="iSubmit" type="button" title="<%- $.trans.gettext('Register'); %>" value="<%- $.trans.gettext('Register'); %>"/></span>
            </form>
        </div>
    </div>
    <div class="dBoardAction dBoardLogin">
        <div class="dFramePng">
            <form method="post" id="fLogin">
                <input type="email" id="iEmail" placeholder="<%- $.trans.gettext('Email'); %>" name="email"/>
                <input type="password" id="iPassword" placeholder="<%- $.trans.gettext('Password'); %>" name="password"/>
                <%
                    if(typeof(msg)!='undefined')
                    {
                        %><label for="iPassword" generated="true" class="error"><%- msg; %></label><%
                    }
                    msg = undefined;
                %>
                <span class="sBtn"><input class="iSubmit" type="button" title="<%- $.trans.gettext('Login'); %>" value="<%- $.trans.gettext('Login'); %>"/></span>
            </form>
        </div>
    </div>
    <div class="dBoardAction dBoardForgot">
        <div class="dFramePng">
            <form method="post" id="fForgot">
                <input type="email" id="iEmail" placeholder="<%- $.trans.gettext('Email'); %>" required name="email"/>
                <canvas id="cCaptcha2" width="100" height="29"></canvas>
                <input type="text" id="iCaptcha" placeholder="<%- $.trans.gettext('Confirm code'); %>" name="captcha"/>
                <span class="sBtn"><input class="iSubmit" type="button" title="<%- $.trans.gettext('Send new password to email'); %>" value="<%- $.trans.gettext('Send new password to email'); %>"/></span>
            </form>
        </div>
    </div>
    <div class="dBoardAction dBoardSetting">
        <div class="dFramePng">
            <div class="dApp dAppLanguages">
                <div class="dNotify">
                    <span class="sBlur" style="background-image: url('../../public/media/images/skin/admin/customer.png')"></span>
                    <span class="sScreen"></span>
                    <span class="sNumber">6</span>
                </div>
                <div class="dClose" board="dBoardLanguages">
                    <span class="sBlur" style="background-image: url('../../public/media/images/skin/admin/language.png')"></span>
                    <span class="sScreen"></span>
                    <span class="fa fa-times"></span>
                </div>
                <a title="<%- $.trans.gettext('Language'); %>">
                    <img src="<%- $.config.skin; %>/admin/language.png" alt="wsup3"/>
                </a>
                <a title="<%- $.trans.gettext('Language'); %>"><%- $.trans.gettext('Language'); %></a>
            </div>
            <div class="dAppAddLanguage"></div>
        </div>
    </div>
    <div class="dBoardAction dBoardLanguages">
        <div class="dFramePng">
            <form method="post" id="fLanguages"></form>
            <span class="sBtnMore" type="language"><input type="button" title="<%- $.trans.gettext('More contents'); %>" value="<%- $.trans.gettext('More contents'); %>"/></span>
        </div>
    </div>
    <div class="dBgAction"></div>
</div>