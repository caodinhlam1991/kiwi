<div class="dBoardAction dBoardOrder">
    <div class="dFramePng">
        <form method="post" id="fOrder"></form>
        <center><span class="sBtnMore" type="order"><input type="button" title="<%- $.trans.gettext('More contents'); %>" value="<%- $.trans.gettext('More contents'); %>"/></span><center>
    </div>
</div>
<div class="dBoardAction dBoardOrderDetail">
    <div class="dFramePng">
        <form method="post" id="fOrderDetail"></form><br/>
        <h3>Tổng tiền: <span class="sOrderPrice"></span> VNĐ</h3>
    </div>
</div>
<div class="dBoardAction dBoardSubscription">
    <div class="dFramePng">
        <form method="post" id="fSubscription"></form>
        <center><span class="sBtnMore" type="subscription"><input type="button" title="<%- $.trans.gettext('More contents'); %>" value="<%- $.trans.gettext('More contents'); %>"/></span></center>
    </div>
</div>
<div class="dBoardAction dBoardContact">
    <div class="dFramePng">
        <form method="post" id="fContact"></form>
        <center><span class="sBtnMore" type="contact"><input type="button" title="<%- $.trans.gettext('More contents'); %>" value="<%- $.trans.gettext('More contents'); %>"/></span></center>
    </div>
</div>
<div class="dBoardAction dBoardCustomers">
    <div class="dFramePng">
        <form method="post" id="fCustomers"></form>
        <center><span class="sBtnMore"  type="customer"><input type="button" title="<%- $.trans.gettext('More contents'); %>" value="<%- $.trans.gettext('More contents'); %>"/></span></center>
    </div>
</div>
<div class="dBoardAction dBoardAddCustomer">
    <div class="dFramePng">
        <form method="post" id="fAddCustomer">
            <div class="dLine">
                <label for="iFullname"><%- $.trans.gettext('Full name'); %>:</label>
                <input type="text" name="fullname" id="iFullname"/>
            </div>
            <div class="dLine">
                <label for="iGender"><%- $.trans.gettext('Gender'); %>:</label>
                <select name="gender" id="iGender" class="iGender">
                    <option value="true"><%- $.trans.gettext('Male'); %></option>
                    <option value="false"><%- $.trans.gettext('Female'); %></option>
                </select>
            </div>
            <div class="dLine">
                <label for="iAddress"><%- $.trans.gettext('Address'); %>:</label>
                <input type="text" name="address" id="iAddress"/>
            </div>
            <div class="dLine">
                <label for="iPhone"><%- $.trans.gettext('Phone'); %>:</label>
                <input type="text" name="phone" id="iPhone"/>
            </div>
            <div class="dLine">
                <label for="iEmail"><%- $.trans.gettext('Email'); %>:</label>
                <input type="text" name="email" id="iEmail"/>
            </div>
            <div class="dPhoto"></div>
            <a class="a1Photo" board="dBoardAddCustomer"><img alt="wsup3" src="<%- $.config.skin; %>/admin/photo.png"/> <%- $.trans.gettext('Add photos'); %></a>
            <a class="aFeature" type="user" num="0" board="dBoardAddLanguage"><%- $.trans.gettext('Add feature'); %></a>
            <div class="dFeatures"></div>
            <div class="dAdvance">
                <div>
                    <select class="iLive">
                        <option value="true"><%- $.trans.gettext('Public'); %></option>
                        <option value="false"><%- $.trans.gettext('Private'); %></option>
                    </select><br/>
                </div>
            </div>
            <a class="aAdvance" title="<%- $.trans.gettext('Advance'); %>"><%- $.trans.gettext('Advance'); %></a>
        </form>
    </div>
</div>