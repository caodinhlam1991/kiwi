<div class="dHeader">
    <div class="dSearch">
        <input class="iSearch" time="1" type="text" autocomplete="yes" placeholder="<%- $.trans.gettext('Search'); %>"/>
        <a class="aHome" onClick="location.reload(true);"><img src="<%- $.config.skin; %>/admin/favicon.ico" alt="wsup3" title="<%- $.trans.gettext('Reload'); %>"/></a>
        <a class="aBack" time="-1" ready="1"><i class="fa fa-caret-left"></i></a>
    </div>
    <div class='dControls'>
        <a title="<%- $.trans.gettext('Exit'); %>" class="aClose"><i class="fas fa-times"></i></a>
        <a title="<%- $.trans.gettext('Smalls'); %>" class="aRestores"><i class="fas fa-window-restore"></i></a>
        <a title="<%- $.trans.gettext('Minimizes'); %>" class="aMinimizes"><i class="fas fa-window-minimize"></i></a>
    </div>
</div>