                                            %>
                                                <p><span><img class="lazy" data-original="<%- $.config.skin; %>/home3.png" alt=""/></span><%- v.detail; %></p>
                                            <%
                                        break;
                                    case 'dien-thoai':
                                            %>
                                                <p><span><img class="lazy" data-original="<%- $.config.skin; %>/phone-icon.png" alt=""/></span><%- v.detail; %></p>
                                            <%
                                        break;
                                    case 'email':
                                            %>
                                                <p><span><img class="lazy" data-original="<%- $.config.skin; %>/email-icon.png" alt=""/></span><%- v.detail; %></p>
                                            <%
                                        break;
                                }
                            });
                        %>
