                    <ul itemscope itemtype="http://www.schema.org/SiteNavigationElement">
                        <%
                            $.each(supports, function(k, v){
                                %>
                                    <li itemprop="name"><a href="<%- $.baseurl(req)+'/'+v.path; %>" title="<%- v.title; %>" itemprop="url"><%- v.title; %></a></li>
                                <%
                            });
                        %>
                    </ul>
                </div>
                <div class="dMenusFooter">
                    <p>Kết nối với chúng tôi</p>
                    <ul>
                        <%
                            $.each(socials, function(k, v){
                                switch(v.path)
                                {
                                    case 'facebook':
                                            %><li><a href="<%- v.detail; %>" title="FACEBOOK">FACEBOOK</a></li><%
                                        break;
                                    case 'google':
                                            %><li><a href="<%- v.detail; %>" title="GOOGLE PLUS">GOOGLE PLUS</a></li><%
                                        break;
                                    case 'twitter':
                                            %><li><a href="<%- v.detail; %>" title="TWITTER">TWITTER</a></li><%
                                        break;
                                }
                            });
                        %>
